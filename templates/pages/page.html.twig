{% extends 'base.html.twig' %}
{% block body %}
    {% if page is defined %}
        {{ page|replace({'#search-form#': block('search_form') })|replace({'#contact-form#': block('contact_form')})|raw }}
    {% endif %}
{% endblock %}
{% block stylesheets %}
    {% if styles is defined %}
        {{ styles|raw }}
    {% endif %}
{% endblock %}
