{% extends 'base.html.twig' %}
{% block body %}
    {% if page is defined %}
        {{ include(template_from_string(page|replace({'#search-form#': block('search_form') })|replace({'#contact-form#': block('contact_form')})|raw)) }}
    {% endif %}
{% endblock %}
{% block stylesheets %}
    {% if styles is defined %}
        {{ include(template_from_string(styles|raw)) }}
    {% endif %}
{% endblock %}
